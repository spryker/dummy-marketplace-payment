{
  "name": "spryker/dummy-marketplace-payment",
  "type": "library",
  "description": "DummyMarketplacePayment module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/kernel": "^3.30.0",
    "spryker/step-engine": "^3.1.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/oms": "*",
    "spryker/payment": "*",
    "spryker/propel": "*",
    "spryker/sales": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/oms": "If you want to use OMS constants.",
    "spryker/payment": "If you want to use Payment command plugins.",
    "spryker/sales": "If you want to use Sales constants."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
